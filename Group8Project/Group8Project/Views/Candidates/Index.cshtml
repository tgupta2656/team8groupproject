@model IEnumerable<Group8Project.Models.CandidateTable>

@{
    ViewBag.Title = "Candidates";
}

<h2>Candidates</h2>


<table class="table">
    <tr>

        <th>

            @Html.DisplayNameFor(model => model.CandidatePicture)

        </th> 

        <th>
            @Html.DisplayNameFor(model => model.CandidateName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CandidateParty)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CandidateAge)
        </th>
        



    </tr>

@foreach (var item in Model) {
    <tr>

        <td>

            <img src="@item.CandidatePicture"/> 

           
        </td> 

        <td>


            <a target="_blank" href=@item.CandidateWebsite> @Html.DisplayFor(modelItem => item.CandidateName) </a>

        </td>

        <td>
            @Html.DisplayFor(modelItem => item.CandidateParty)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CandidateAge)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CandidateGender)
        </td>




    </tr>
}

</table>
